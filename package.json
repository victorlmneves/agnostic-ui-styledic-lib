{
  "name": "agnostic-ui-styledic-lib",
  "private": false,
  "version": "0.0.8",
  "main": "./dist/agnostic.umd.js",
  "module": "./dist/agnostic.es.js",
  "exports": {
    ".": {
      "import": "./dist/agnostic.es.js",
      "require": "./dist/agnostic.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": ["dist"],
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "prepare": "tsc",
    "build": "npm run styledic && vite build && vue-tsc --emitDeclarationOnly && cp dist/src dist/types",
    "styledic": "brand=yello style-dictionary build --config \"./styleDictionary/config.js\"",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "vue": "3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "2.2.0",
    "@vuepress/plugin-search": "^2.0.0-beta.38",
    "postcss": "8.4.12",
    "postcss-calc": "8.2.4",
    "postcss-inline-svg": "5.0.0",
    "postcss-preset-env": "7.4.3",
    "postcss-scss": "4.0.3",
    "postcss-simple-vars": "6.0.3",
    "rollup-plugin-copy": "3.4.0",
    "sass": "1.49.10",
    "style-dictionary": "3.7.0",
    "typescript": "4.5.4",
    "vite": "^2.9.1",
    "vue-tsc": "0.29.8",
    "vuepress": "^2.0.0-beta.38",
    "vuepress-plugin-copy-code2": "2.0.0-beta.36"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/victorlmneves/agnostic-ui-styledic-lib.git"
  },
  "author": "Victor Neves",
  "license": "MIT"
}
